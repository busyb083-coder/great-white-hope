{
  "name": "@great-white-hope/backend",
  "version": "1.0.0",
  "description": "Great White Hope - Backend API",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && node -r esbuild-register dist/index.js",
    "start": "node dist/index.js",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "seed": "tsx src/seed.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "@prisma/client": "^5.7.0",
    "jsonwebtoken": "^9.1.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "aws-sdk": "^2.1500.0",
    "nodemailer": "^6.9.7",
    "express-rate-limit": "^7.1.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "@sentry/node": "^7.84.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.14",
    "prisma": "^5.7.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "esbuild-register": "^3.5.0"
  }
}
